pipeline {
    agent {
  label 'labk8z'
}
  stages{

    
    
    stage('Deploy step') {
        environment {
            MONGO_URL = credentials('mongo-url')
            DOCKER_CREDS= credentials('github_id')
            }
        steps {
            sh '''
            docker login -u $DOCKER_CREDS_USR -p $DOCKER_CREDS_PSW
            docker compose pull
            docker compose up -d 
            docker logout
            env
            '''
        }
    }
  } 
} 
